<script>
$(document).ready(function() {
    var totalRecords = {{ totalRecords|default(0) }};
    var table = $('#myTable').DataTable({
        language: {
            "paging": true,
            "sProcessing": "Traitement en cours...",
            "sLengthMenu": "Afficher _MENU_ lignes",
            "sZeroRecords": "Aucun résultat trouvé",
